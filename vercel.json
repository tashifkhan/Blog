{
	"buildCommand": "bun run build",
	"installCommand": "bun install",
	"framework": "astro",
	"routes": [
		{
			"handle": "filesystem"
		},
		{
			"src": "/(.*)",
			"status": 404,
			"dest": "https://tashif.codes"
		}
	],
	"rewrites": [
		{
			"source": "/ph/static/(.*)",
			"destination": "https://eu-assets.i.posthog.com/static/$1"
		},
		{
			"source": "/ph/(.*)",
			"destination": "https://eu.i.posthog.com/$1"
		}
	]
}
